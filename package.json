{
  "name": "ai-developer-analytics",
  "type": "commonjs",
  "displayName": "AI Developer Analytics",
  "description": "Hybrid AI developer analytics extension with dual-level routing, local Ollama support, and adaptive coaching - Built: 2025-10-07 07:14 CEST",
  "version": "0.3.1",
  "license": "AGPL-3.0-only",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/ai-developer-analytics"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-analytics",
          "title": "AI Analytics",
          "icon": "$(graph)"
        }
      ]
    },
    "views": {
      "ai-analytics": [
        {
          "id": "ai-command-bar",
          "name": "AI Command Bar",
          "type": "webview"
        },
        {
          "id": "ai-dashboard",
          "name": "AI Dashboard",
          "type": "webview"
        },
        {
          "id": "ai-coach",
          "name": "AI Coach",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "ai-analytics.openCommandBar",
        "title": "Open AI Command Bar",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.openDashboard",
        "title": "Open AI Dashboard",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.openCoach",
        "title": "Open AI Coach",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.toggleHotReload",
        "title": "Toggle Hot Reload",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.testDeepSeek",
        "title": "Test DeepSeek API",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.startMCPServer",
        "title": "Start MCP Server",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.stopMCPServer",
        "title": "Stop MCP Server",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.restartMCPServer",
        "title": "Restart MCP Server",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.showMCPServerStatus",
        "title": "Show MCP Server Status",
        "category": "AI Analytics"
      },
      {
        "command": "ai-analytics.selectModel",
        "title": "Select AI Model",
        "category": "AI Analytics"
      }
    ],
    "configuration": {
      "title": "AI Developer Analytics",
      "properties": {
        "aiAnalytics.ollamaEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable Ollama local AI support"
        },
        "aiAnalytics.ollamaUrl": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "Ollama API URL"
        },
        "aiAnalytics.defaultOllamaModel": {
          "type": "string",
          "default": "phi-4",
          "description": "Default Ollama model"
        },
        "aiAnalytics.routingMode": {
          "type": "string",
          "default": "auto-local",
          "enum": [
            "direct",
            "auto-local",
            "auto-ollama",
            "auto-gpt5",
            "auto-claude",
            "auto-deepseek"
          ],
          "description": "AI routing mode"
        },
        "aiAnalytics.telemetryEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable local telemetry collection"
        },
        "aiAnalytics.hotReloadEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable hot reload for UI and backend"
        },
        "aiAnalytics.deepseekApiKey": {
          "type": "string",
          "default": "",
          "description": "DeepSeek API key"
        },
        "aiAnalytics.commandBarFontFamily": {
          "type": "string",
          "default": "var(--vscode-editor-font-family)",
          "description": "Font family for the AI Command Bar interface"
        },
        "aiAnalytics.commandBarFontSize": {
          "type": "number",
          "default": 13,
          "description": "Base font size for the AI Command Bar interface"
        },
        "aiAnalytics.chatFontSize": {
          "type": "number",
          "default": 13,
          "description": "Font size for chat messages in the conversation area"
        },
        "aiAnalytics.aiResponseFontSize": {
          "type": "number",
          "default": 13,
          "description": "Font size for AI responses in the conversation"
        },
        "aiAnalytics.codeBlockFontSize": {
          "type": "number",
          "default": 12,
          "description": "Font size for code blocks and quoted text"
        },
        "aiAnalytics.inlineCodeFontSize": {
          "type": "number",
          "default": 12,
          "description": "Font size for inline code snippets"
        },
        "aiAnalytics.inputFontSize": {
          "type": "number",
          "default": 14,
          "description": "Font size for the input textarea"
        },
        "aiAnalytics.dropdownFontSize": {
          "type": "number",
          "default": 11,
          "description": "Font size for dropdown menus and controls"
        },
        "aiAnalytics.coachFontSize": {
          "type": "number",
          "default": 13,
          "description": "Font size for coaching advice section"
        },
        "aiAnalytics.metricsFontSize": {
          "type": "number",
          "default": 9,
          "description": "Font size for metrics display"
        },
        "aiAnalytics.defaultEngine": {
          "type": "string",
          "default": "deepseek",
          "enum": [
            "auto",
            "deepseek",
            "openai",
            "anthropic",
            "ollama"
          ],
          "description": "Default AI engine selection"
        },
        "aiAnalytics.defaultTaskType": {
          "type": "string",
          "default": "general",
          "enum": [
            "general",
            "code",
            "documentation",
            "debug"
          ],
          "description": "Default task type selection"
        },
        "aiAnalytics.defaultMode": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto",
            "eco",
            "normal",
            "quality",
            "strict-json",
            "creative"
          ],
          "description": "Default mode selection"
        },
        "aiAnalytics.accentColor": {
          "type": "string",
          "default": "#5B7FED",
          "description": "Accent color for the interface (hex color code)"
        },
        "aiAnalytics.showMetrics": {
          "type": "boolean",
          "default": true,
          "description": "Show metrics section in the command bar"
        },
        "aiAnalytics.coachEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI coaching advice"
        },
        "aiAnalytics.coachCollapsedByDefault": {
          "type": "boolean",
          "default": true,
          "description": "Collapse coach advice section by default"
        },
        "aiAnalytics.sessionTabsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable session tabs functionality"
        },
        "aiAnalytics.autoExpandTextarea": {
          "type": "boolean",
          "default": true,
          "description": "Auto-expand textarea based on content"
        },
        "aiAnalytics.streamingEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable real-time streaming responses"
        },
        "aiAnalytics.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API key"
        },
        "aiAnalytics.anthropicApiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic Claude API key"
        },
        "aiAnalytics.moonshotApiKey": {
          "type": "string",
          "default": "",
          "description": "Moonshot (Kimi) API key"
        },
        "aiAnalytics.moonshotDefaultModel": {
          "type": "string",
          "default": "moonshot-v1-8k",
          "description": "Default Moonshot model (free text, suggestions available)"
        }
      }
    }
  },
  "workspaces": [
    ".",
    "server-mcp"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run build:mcp && npm run build:frontend",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "package": "node update-build-date.js && vsce package",
    "publish": "vsce publish",
    "build:mcp": "cd server-mcp && npm run build",
    "test:mcp": "cd server-mcp && npm test",
    "build:frontend": "tsup --config tsup.config.js",
    "watch:frontend": "tsup --config tsup.config.js --watch"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "20.x",
    "@types/sql.js": "^1.4.9",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^2.0.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^9.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.25.0",
    "axios": "^1.12.2",
    "crypto-js": "^4.2.0",
    "lru-cache": "^10.0.0",
    "openai": "^4.0.0",
    "sql.js": "^1.13.0"
  }
}